<div class="tab-pane fade" id="show-activity">
    <!-- DataTales Example -->
    <div class="card shadow mb-4 mt-2">
        <div class="card-body">
            <div class="row">
                <div class="col-md-5">
                    <form action="/admin/bank" method="POST" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="nama">Nama</label>
                            <input type="text" class="form-control" name="nama" placeholder="Masukan nama" required>
                        </div>
                        <div class="form-group">
                            <label for="nama">Type</label>
                            <input type="number" class="form-control" name="type" placeholder="Masukan qty" required>
                        </div>
                        <div class="form-group">
                            <label for="nama">Image</label>
                            <input type="file" class="form-control" name="image" required>
                        </div>

                        <button type="submit" class="btn btn-primary">save</button>

                    </form>
                </div>
                <div class="col-md-7">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="dataTableActivity" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>No</th>
                                    <th>Nama</th>
                                    <th>Type</th>
                                    <th>Image URL</th>
                                    <th>Aksi</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
  </div>
  